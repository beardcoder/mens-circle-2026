# Payload CMS API URL
CMS_URL=http://localhost:3001

# Payload API Key (generate in CMS Admin → Users → your user → API Key)
PAYLOAD_API_KEY=

# Site URL (for canonical URLs and OG tags)
SITE_URL=http://localhost:4400

# Revalidation secret (must match the secret in Payload CMS env)
REVALIDATE_SECRET=change-this-to-a-random-secret-matching-cms

# Umami Analytics (https://umami.is)
PUBLIC_UMAMI_ENABLED=false
PUBLIC_UMAMI_WEBSITE_ID=
PUBLIC_UMAMI_SCRIPT_URL=https://cloud.umami.is/script.js

